<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="8b812371-1829-479f-9989-7ed82b7298c6" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Build</name>
  <description />
  <settings>
    <options>
      <option name="publishArtifactCondition" value="SUCCESSFUL" />
    </options>
    <parameters>
      <param name="maven_goals" value="clean test" />
    </parameters>
    <build-runners>
      <runner id="Maven2" name="Maven" type="Maven2">
        <parameters>
          <param name="goals" value="%maven_goals%" />
          <param name="localRepoScope" value="agent" />
          <param name="maven.path" value="%teamcity.tool.maven.DEFAULT%" />
          <param name="pomLocation" value="pom.xml" />
          <param name="teamcity.step.mode" value="default" />
          <param name="userSettingsSelection" value="settings.xml" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="NetologyExampleTeamcity_HttpsGithubComTundravargNetologyExampleTeamcityGitRefsHeadsMaster" />
    </vcs-settings>
    <requirements />
    <build-triggers>
      <build-trigger id="TRIGGER_1" type="vcsTrigger">
        <parameters>
          <param name="branchFilter" value="+:&lt;default&gt;" />
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
        <build-customization enforceCleanCheckout="true">
          <parameters>
            <param name="maven_goals" value="clean deploy" />
          </parameters>
        </build-customization>
      </build-trigger>
      <build-trigger id="TRIGGER_2" type="vcsTrigger">
        <parameters>
          <param name="branchFilter"><![CDATA[+:*
-:<default>]]></param>
          <param name="enableQueueOptimization" value="true" />
          <param name="quietPeriodMode" value="DO_NOT_USE" />
        </parameters>
        <build-customization>
          <parameters>
            <param name="maven_goals" value="clean test" />
          </parameters>
        </build-customization>
      </build-trigger>
    </build-triggers>
    <build-extensions>
      <extension id="perfmon" type="perfmon">
        <parameters>
          <param name="teamcity.perfmon.feature.enabled" value="true" />
        </parameters>
      </extension>
    </build-extensions>
    <cleanup />
  </settings>
</build-type>

